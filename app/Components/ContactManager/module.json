{
    "enabled": true,
    "info": {
        "name": "Contact Manager",
        "alias": "ContactManager",
        "version": "0.1",
        "author": "Doptor",
        "website": "http://www.doptor.org"
    },
    "target": "admin|backend",
    "forms": [
        {
            "form_id": "18",
            "form_name": "All Contacts",
            "table": "contact_details",
            "fields": [
                "name",
                "alias",
                "image",
                "email",
                "address",
                "city",
                "state",
                "zip_code",
                "country",
                "telephone",
                "mobile",
                "fax",
                "website",
                "location"
            ],
            "field_names": [
                "Name",
                "Alias",
                "Image",
                "Email",
                "Address",
                "City",
                "State",
                "Postal / ZIP code",
                "Country",
                "Telephone",
                "Mobile",
                "Fax",
                "Website",
                "Location"
            ],
            "model": "ContactDetail"
        },
        {
            "form_id": "20",
            "form_name": "Recent Emails",
            "table": "contact_emails",
            "fields": [
                "name",
                "email",
                "subject",
                "message"
            ],
            "field_names": [
                "Sender Name",
                "Sender Email",
                "Subject",
                "Message"
            ],
            "model": "ContactEmail"
        }
    ]
}
